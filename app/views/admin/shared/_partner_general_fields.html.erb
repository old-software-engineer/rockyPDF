<fieldset>
  <legend>General</legend>
  <ul class="stacked">
    <li class="width_b">
      <h3><%= f.label :name %></h3>
      <%= field_div f, :name %>
      <span class="error"><%= @partner.errors[:name].join("\n") %></span>
    </li>
    <li class="width_a">
      <h3><%= f.label :organization %></h3>
      <%= field_div f, :organization %>
      <span class="error"><%= @partner.errors[:organization].join("\n") %></span>
    </li>
    <li class="width_a">
      <h3><%= f.label :url %></h3>
      <%= field_div f, :url %>
      <span class="error"><%= f.object.errors[:url].join("\n") %></span>
    </li>
    
    <li class="width_a">
      <h3><%= f.label :email %></h3>
      <%= field_div f, :email %>
      <span class="error"><%= @partner.errors[:email].join("\n") %></span>
    </li>

    <li class="width_a">
      <h3><%= f.label :username %></h3>
      <%= field_div f, :username %>
      <span class="error"><%= @partner.errors[:username].join("\n") %></span>
    </li>

    <li class="width_a">
      <h3><%= f.label :phone %></h3>
      <%= field_div f, :phone %>
      <span class="error"><%= f.object.errors[:phone].join("\n") %></span>
    </li>
     
  </ul>
  
  <ul class="stacked">
    <li class="width_a">
      <h3><%= f.label :address %></h3>
      <%= field_div f, :address %>
      <span class="error"><%= f.object.errors[:address].join("\n") %></span>
    </li>
  </ul>
  <ul class="stacked">
    <li class="width_a">
      <h3><%= f.label :city %></h3>
      <%= field_div f, :city %>
      <span class="error"><%= f.object.errors[:city].join("\n") %></span>
    </li>
    <li class="width_b">
      <h3><%= f.label :state_abbrev, "State" %></h3>
      <div><%= f.select :state_abbrev, GeoState.collection_for_select, :include_blank => "select a state" %></div>
      <span class="error"><%= f.object.errors[:state_abbrev].join("\n") %></span>
    </li>
    <li class="width_c">
      <h3><%= f.label :zip_code %></h3>
      <%= field_div f, :zip_code %>
      <span class="error"><%= f.object.errors[:zip_code].join("\n") %></span>
    </li>
  </ul>
  
  <ul class="checkbox">
    <li>
      <h3>
        <%= f.check_box :active %>
        <%= f.label :active, 'Active user (allowed to login)' %>
      </h3>      
    </li>
    <li>
      <h3>
        <%= f.check_box :enabled_for_grommet %>
        <%= f.label :enabled_for_grommet, 'Enabled For Grommet' %>
        <br/>
        <%= f.label :pa_api_key %>
        <%= field_div f, :pa_api_key %>
        <span class="error"><%= @partner.errors[:pa_api_key].join("\n") %></span>
      </h3>
    </li>
    <li>
      <h3>
      </h3>
    </li>
  </ul>
    
</fieldset>
